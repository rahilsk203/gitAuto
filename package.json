{
  "name": "@rahilsk/gitauto",
  "version": "1.6.3",
  "description": "A Node.js CLI tool for automating common GitHub repository management tasks",
  "main": "index.js",
  "bin": {
    "gitauto": "./index.js"
  },
  "files": [
    "*.js",
    "lib/",
    "scripts/"
  ],
  "scripts": {
    "start": "node index.js",
    "build": "node scripts/build.js",
    "test": "npx mocha test/*.test.js",
    "test:core": "npx mocha test/core.test.js",
    "test:optimizations": "node test/optimizations.test.js",
    "test:caching": "node test/caching.test.js",
    "test:batch": "node test/batch.test.js",
    "test:integration": "node test/integration.test.js"
  },
  "keywords": [
    "git",
    "github",
    "automation",
    "cli",
    "developer-tools"
  ],
  "author": "SKRahil <rahilsk203@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "inquirer": "^8.2.0",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "sinon": "^15.0.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rahilsk203/gitAuto.git"
  },
  "bugs": {
    "url": "https://github.com/rahilsk203/gitAuto/issues"
  },
  "homepage": "https://github.com/rahilsk203/gitAuto#readme"
}
